            text = "Вот я говорю, где я там живу на севере, там вообще, раньше когда мы приехали туда 45 лет тому назад, там были еще бабушки какрельские которые, карелочки, и то у них диалект был карельский ближе, больше похож на финский."
        intervals [61]
            xmin = 3089.5
            xmax = 3089.59
            text = ""
        intervals [62]
            xmin = 3089.59
            xmax = 3094.85
            text = "То есть они вот говорили, я что-то их понимала, ну по смыслу только, но не все."
        intervals [63]
            xmin = 3094.85
            xmax = 3094.89
            text = ""
        intervals [64]
            xmin = 3094.89
            xmax = 3102.06
            text = "А разговор между нами не ладился, я говорила на своем южном диалекте, а они на своем северном, ближе к финнам."
        intervals [65]
            xmin = 3102.06
            xmax = 3102.425
            text = ""
        intervals [66]
            xmin = 3102.425
            xmax = 3106.04
            text = "Они меня тоже не все слова понимали."
        intervals [67]
            xmin = 3106.04
            xmax = 3110.367
            text = ""
        intervals [68]
            xmin = 3110.367
            xmax = 3117.28
            text = "Да, вот, и вот я уезжаю и приезжаю сюда летом, ну один месяц какой-то июнь, июль и август."
        intervals [69]
            xmin = 3117.28
            xmax = 3117.333
            text = ""
        intervals [70]
            xmin = 3117.333
            xmax = 3121.373
            text = "Я единственный месяц в году говорю на карельском языке."
        intervals [71]
            xmin = 3121.373
            xmax = 3121.403
            text = ""
        intervals [72]
            xmin = 3121.403
            xmax = 3130.61
            text = "Потому что там не с кем разговаривать, все старые бабушки, с кем можно было, они умерли уже, вот, и там очень много семей с этой, с [нрзб]."
        intervals [73]
            xmin = 3130.61
            xmax = 3130.656
            text = ""
        intervals [74]
            xmin = 3130.656
            xmax = 3133.31
            text = "Вот они чисто говорят так как мы с вами говорим"
        intervals [75]
            xmin = 3133.31
            xmax = 3133.341
            text = ""
        intervals [76]
            xmin = 3133.341
            xmax = 3138.22
            text = "Ну они там тоже, там [нрзб], только вот мы с ней можем, и то."
        intervals [77]
            xmin = 3138.22
            xmax = 3143.37
            text = "Я прихожу к ней иногда в гости, я говорю на карельском, но она мне отвечает на русском."
        intervals [78]
            xmin = 3143.37
            xmax = 3143.795
            text = ""
        intervals [79]
            xmin = 3143.795
            xmax = 3144.995
            text = "Привыкла."
        intervals [80]
            xmin = 3144.995
            xmax = 3145.945
            text = ""
        intervals [81]
            xmin = 3145.945
            xmax = 3153.53
            text = "Или на работе иногда что-нибудь такое, это, выскочит у меня слово карельское среди русского, ну простоыот выскочит что-то."
        intervals [82]
            xmin = 3153.53
            xmax = 3153.537
            text = ""
        intervals [83]
            xmin = 3153.537
            xmax = 3156.26
            text = "Что, что вы сказали? Ой, говорю."
        intervals [84]
            xmin = 3156.26
            xmax = 3188.991
            text = ""
        intervals [85]
            xmin = 3188.991
            xmax = 3191.803
            text = "Но теперь как бы учить уже - никого мы не учим."
        intervals [86]
            xmin = 3191.803
            xmax = 3192.061
            text = ""
        intervals [87]
            xmin = 3192.061
            xmax = 3195.652
            text = "Просто если я знаю, предположим, что она знает карельский."
        intervals [88]
            xmin = 3195.652
            xmax = 3196.07
            text = ""
        intervals [89]
            xmin = 3196.07
            xmax = 3199.426
            text = "Но я разговариваю на карельском, но она мне отвечает на русском."
        intervals [90]
            xmin = 3199.426
            xmax = 3199.733
            text = ""
        intervals [91]
            xmin = 3199.733
            xmax = 3207.393
            text = "Он говорит: ""Я забыла!"" Но как можно забыть? Я вот могу целый год молчать, приезжаю домой, разговариваю, как будтно никуда не уезжала"
        intervals [92]
            xmin = 3207.393
            xmax = 3207.397
            text = ""
        intervals [93]
            xmin = 3207.397
            xmax = 3210.984
            text = "Невезможно забыть свой язык, который та знал с рождения."
        intervals [94]
            xmin = 3210.984
            xmax = 3245.951
            text = ""
        intervals [95]
            xmin = 3245.951
            xmax = 3249.426
            text = "Вот одна из них я. Я пошла в школу, русского языка вообше не знала."
        intervals [96]
            xmin = 3249.426
            xmax = 3250.116
            text = ""
        intervals [97]
            xmin = 3250.116
            xmax = 3251.903
            text = "В шесть= шестьдесят четвертом."
        intervals [98]
            xmin = 3251.903
            xmax = 3265.567
            text = ""
        intervals [99]
            xmin = 3265.567
            xmax = 3274.0
            text = "Единнственно только на спасало то, что сама учительница была тоже карелка. Она понимала карельский язык, поэтоей было проще нас научить."
        intervals [100]
            xmin = 3274.0
            xmax = 3291.427
            text = ""
        intervals [101]
            xmin = 3291.427
            xmax = 3295.443
            text = "А вообще вот я вот всегда говорю иногда, но не всем..."
        intervals [102]
            xmin = 3295.443
            xmax = 3304.812
            text = ""
        intervals [103]
            xmin = 3304.812
            xmax = 3315.426
            text = "Молодая же была, глупая, сегодня я б промолчала. А когда узнали, что я карелка, знаете, там вот, в основном, белоруссы, украинцы вербованные."
        intervals [104]
            xmin = 3315.426
            xmax = 3315.666
            text = ""
        intervals [105]
            xmin = 3315.666
            xmax = 3322.338
            text = "И было такое противостояние именно вот нелюбовь, потому что ты карелка, ты вообще..."
        intervals [106]
            xmin = 3322.338
            xmax = 3327.581
            text = ""
        intervals [107]
            xmin = 3327.581
            xmax = 3333.623
            text = "А моя... Мой сын женился на... это, девушка. Она выросла в Костомукше, родилась и выросла. "
        intervals [108]
            xmin = 3333.623
            xmax = 3333.926
            text = ""
        intervals [109]
            xmin = 3333.926
            xmax = 3339.213
            text = "Потом когда мы со сватьей познакомились, и как-то сватья говорит, прошло уже сколько времени. "
        intervals [110]
            xmin = 3339.213
            xmax = 3342.088
            text = ""
        intervals [111]
            xmin = 3342.088
            xmax = 3350.246
            text = "И она как-то говорит, что ""А в детстве нам родители запрещали общаться с детьми из карельских семей"""
        intervals [112]
            xmin = 3350.246
            xmax = 3351.661
            text = ""
        intervals [113]
            xmin = 3351.661
            xmax = 3354.164
            text = "Вот такое было противостояние раньше."
        intervals [114]
            xmin = 3354.164
            xmax = 3356.146
            text = ""
        intervals [115]
            xmin = 3356.146
            xmax = 3359.82
            text = "Ну вот почему-то да. Вот было такое."
        intervals [116]
            xmin = 3359.82
            xmax = 3370.18
            text = ""
        intervals [117]
            xmin = 3370.18
            xmax = 3377.131
            text = "Прямо ладошкой проведешь - они прямо как зеркало. Я помню, сама натирала голяком."
        intervals [118]
            xmin = 3377.131
            xmax = 3385.456
            text = ""
        intervals [119]
            xmin = 3385.456
            xmax = 3390.475
            text = "Ну, в разных национальностях есть разные люди, поэтому..."
        intervals [120]
            xmin = 3390.475
            xmax = 3399.051
            text = ""
        intervals [121]
            xmin = 3399.051
            xmax = 3404.443
            text = "А потом, конечно, когда они меня узнали, когда я уже там, потом, конечно, уже это."
        intervals [122]
            xmin = 3404.443
            xmax = 3418.748
            text = ""
        intervals [123]
            xmin = 3418.748
            xmax = 3427.525
            text = "Даже, даже я замуж когда за Колю вышла, он с Беларуси приехал. Даже моя свекровь со свекором тоже как бы меня вот не приняли в семью."
        intervals [124]
            xmin = 3427.525
            xmax = 3427.579
            text = ""
        intervals [125]
            xmin = 3427.579
            xmax = 3431.803
            text = "Потому что карелка - не человек. Сына забарала."
        intervals [126]
            xmin = 3431.803
            xmax = 3433.331
            text = ""
        intervals [127]
            xmin = 3433.331
            xmax = 3435.738
            text = "И так у них, у нас отношения не сложились."
        intervals [128]
            xmin = 3435.738
            xmax = 3452.041
            text = ""
        intervals [129]
            xmin = 3452.041
            xmax = 3455.147
            text = "Жизнью мы своей доказали, что мы люди. Да еще какие, да?"
        intervals [130]
            xmin = 3455.147
            xmax = 3457.58
            text = ""
        intervals [131]
            xmin = 3457.58
            xmax = 3462.06
            text = "Это как бы из лирики уже. Как бы это все в прошлом уже, но все равно помним."
        intervals [132]
            xmin = 3462.06
            xmax = 3473.245
            text = ""
        intervals [133]
            xmin = 3473.245
            xmax = 3477.18
            text = "В Гавриловке вам показывали дом, где бабушка-сказительница жила?"
        intervals [134]
            xmin = 3477.18
            xmax = 3477.4
            text = ""
        intervals [135]
            xmin = 3477.4
            xmax = 3478.86
            text = "Вот это моя бабушка."
        intervals [136]
            xmin = 3478.86
            xmax = 3480.24
            text = ""
        intervals [137]
            xmin = 3480.24
            xmax = 3488.94
            text = "Бабушки, моей бабушки, как бы, как сказать, моей бабушки. Вот моя бабушка Гавриловская...  "
        intervals [138]
            xmin = 3488.94
            xmax = 3489.72
            text = ""
        intervals [139]
            xmin = 3489.72
            xmax = 3490.175
            text = "Да."
        intervals [140]
            xmin = 3490.175
            xmax = 3491.46
            text = ""
        intervals [141]
            xmin = 3491.46
            xmax = 3497.96
            text = "Моя бабушка и бабы Кати мужья были братьями, то есть это родвенница была - баба Катя."
        intervals [142]
            xmin = 3497.96
            xmax = 3501.64
            text = ""
        intervals [143]
            xmin = 3501.64
            xmax = 3505.0
            text = "В детстве у нее ночевали - она все сказки рассказывала, правда..."
