    <TIER LINGUISTIC_TYPE_REF="note" PARENT_REF="Text-txt-ynk" TIER_ID="Translation-gls-ru">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a33" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>В 1956-м году в Наукан привезли памятник Дежневу</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a34" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>ящик сначала</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a35" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>деревянные ящик а потом сверху</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a36" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>сделали ящик из железа</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a37" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>очень тяжелый был</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>когда наступило лето на пароходе привезли этот памятник дежневу</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>напротив полярной станции как бы сделали наверх дорогу из бревен</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>веревками этот ящик</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a42" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>тянут обвив ремнями</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a43" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>и русские, и науканцы</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a45" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>а науканцы как-то еще тогда плохо понимали русский язык</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a46" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>не говорили</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a47" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>только эти молодые люди и школьники (молодежь) по-русски</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a48" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>научались говорить по-русски</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a49" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>а, но мужчины и старики плохо...</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a52" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>...говорили и не очень понимали</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a54" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>это тянут наверх по настилу (по бревнам) = русские кричат</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a55" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>а науканцы-то по-другому слышат</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a56" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>слышат арва сирак арва сирак</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a57" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE>наверх подняли вот этот памятник = и перед маяком поставили этот памятник</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a58" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE>один науканец однажды увидел земляка (тоже живущего в Наукане)</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a59" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE>откуда у тебя такой нож?!</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a60" ANNOTATION_REF="a29">
                <ANNOTATION_VALUE>науканец другой сказал</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a61" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE>из куска арвасирака я сделал этот нож</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a62" ANNOTATION_REF="a31">
                <ANNOTATION_VALUE>вот так вот раз два взяли слышали арвасирак</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a63" ANNOTATION_REF="a32">
                <ANNOTATION_VALUE>и это железо он назвал арвасирой</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
